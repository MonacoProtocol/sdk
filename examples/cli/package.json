    "bs58": "^4.0.1",
    "typescript": "^4.5.4",
    "ts-node": "^10.7.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
